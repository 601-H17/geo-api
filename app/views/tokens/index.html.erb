<div class="container">
  <h1>Tokens</h1>

  <% @tokens.each do |token| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-sm-12 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-3">

          <li><%= token.access_token %></li>

        </div>
      </div>
    </ul>
  <% end %>
</div>